{
  "expo": {
    "name": "islamic_watch",
    "slug": "islamic_watch",
    "scheme": "myapp",
    "description": "React Native - Soft UI Kit - Free",
    "primaryColor": "#CB0C9F",
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ]
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#CB0C9F"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.islamicwatch.test",
      "infoPlist": {
        "LSApplicationQueriesSchemes": [
          "lyft"
        ]
      }
    },
    "android": {
      "package": "com.islamicwatch.test",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#CB0C9F"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "packagerOpts": {
      "sourceExts": [
        "cjs"
      ]
    },
    "extra": {
      "eas": {
        "projectId": "14de3e55-b865-46da-898c-84462e617300"
      }
    }
  }
}
